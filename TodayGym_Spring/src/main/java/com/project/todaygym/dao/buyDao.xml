<?xml version="1.0" encoding="UTF-8"?>
<!-- ﻿Mybatis mapper를 작성하기 위한 문서타입(DTD)설정 -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- mapper : interface에 정의한 SQL 실행 메소드와 mybatis의 태그요소를 연결. -->
<mapper namespace="com.project.todaygym.dao.buyDao">

	<!-- 1. 회원권 정보 가져오기 -->
	<select id="buySelect" parameterType="String" resultType="com.project.todaygym.dto.buyDto">
		SELECT * FROM buy_tb WHERE b_code = #{b_code}
	</select>
	
	
</mapper>